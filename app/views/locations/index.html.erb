<div class="org-index text-center">
  <h1><%=@org.name%></h1>
  <h3>Locations</h13>
  <br>
  <br>
  <h4><%= link_to "create a location", new_organization_location_path(@org) %></h4>
  <div class="container org-container">
    <br>
    <ul>
      <% @locations.each do |loc| %>
        <div class="col-lg-4 col-md-3">
          <div class="panel">
            <%= link_to edit_location_path(loc) do %>
              <div id="loc-name"><%= loc.city %></div>
            <% end %>
          </div>
        </div>
      <% end %>
    </ul>
  </div>
</div>  